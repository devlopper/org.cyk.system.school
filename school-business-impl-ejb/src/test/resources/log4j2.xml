		</Logger>
		
		<Logger name="org.cyk.system.school.business.impl.session.ClassroomSessionBusinessImpl" level="trace">
			<AppenderRef ref="Console" />
		</Logger>
		<!--Logger name="org.cyk.system.school.business.impl.session.ClassroomSessionDivisionBusinessImpl" level="trace">
			<AppenderRef ref="Console" />
		</Logger-->
